{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"include": ["./src/client.js"]
	},
	"sourceType": "module",
	"plugins": ["./node_modules/jsdoc-export-default-interop/dist/index"],
	"opts": {
		"encoding": "utf8",
		"destination": "./doc/"
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": true
		}
	}
}
